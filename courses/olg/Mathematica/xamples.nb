   RowBox[{
    RowBox[{"f", "[", 
     RowBox[{"k", ",", "\[Alpha]", ",", "\[Rho]"}], "]"}], "-", 
    RowBox[{
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{"f", "[", 
        RowBox[{"k", ",", "\[Alpha]", ",", "\[Rho]"}], "]"}], ",", "k"}], 
      "]"}], "*", "k"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
