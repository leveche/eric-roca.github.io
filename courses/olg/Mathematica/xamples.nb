             RowBox[{"1", "/", 
              RowBox[{"(", 
               RowBox[{"1", "+", "n"}], ")"}]}], "*", 
             RowBox[{"savings", "[", 
              RowBox[{"w", ",", "R", ",", "\[Sigma]", ",", "\[Beta]"}], 
              "]"}]}]}], "/.", 
