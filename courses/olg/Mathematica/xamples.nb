Cell[BoxData[
 RowBox[{
  RowBox[{"stability", "[", 
   RowBox[{
   "\[Alpha]_", ",", "\[Beta]_", ",", "\[Sigma]_", ",", "\[Rho]_", ",", 
    "n_"}], "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Length", "[", 
