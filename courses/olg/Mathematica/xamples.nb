        RowBox[{"(", 
         RowBox[{"1", "-", "\[Alpha]"}], ")"}]}], ")"}], "^", 
      RowBox[{"(", 
       RowBox[{"1", "/", "\[Rho]"}], ")"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"u", "[", 
    RowBox[{"x_", ",", "\[Sigma]_"}], "]"}], "=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"\[Sigma]", "\[Equal]", "1"}], ",", 
     RowBox[{"Log", "[", "x", "]"}], ",", 
     RowBox[{
      RowBox[{"x", "^", 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{"1", "/", "\[Sigma]"}]}], ")"}]}], "/", 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{"1", "/", "\[Sigma]"}]}], ")"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"wfunction", "[", 
