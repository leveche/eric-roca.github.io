          RowBox[{"k", "\[Equal]", 
           RowBox[{
            RowBox[{"1", "/", 
             RowBox[{"(", 
              RowBox[{"1", "+", "n"}], ")"}]}], "*", 
            RowBox[{"savings", "[", 
             RowBox[{"w", ",", "R", ",", "\[Sigma]", ",", "\[Beta]"}], 
             "]"}]}]}], "/.", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"w", "\[Rule]", " ", 
             RowBox[{"wfunction", "[", 
              RowBox[{"k", ",", "\[Alpha]", ",", "\[Rho]"}], "]"}]}], ",", 
            RowBox[{"R", "\[Rule]", " ", 
             RowBox[{"Rfunction", "[", 
              RowBox[{"k", ",", "\[Alpha]", ",", "\[Rho]"}], "]"}]}]}], 
           "}"}]}], ",", "k", ",", "Reals"}], "]"}]}]}], "]"}], ";", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "1"}], ",", 
