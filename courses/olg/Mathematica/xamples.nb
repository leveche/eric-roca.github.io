           "\[Sigma]", ",", "\[Rho]", ",", "n"}], "]"}], ",", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"-", "1"}], "<", 
            RowBox[{"der", "[", 
