   RowBox[{"f", "[", 
    RowBox[{"k_", ",", "\[Alpha]_", ",", "\[Rho]_"}], "]"}], "=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"\[Rho]", "\[Equal]", "0"}], ",", 
     RowBox[{"k", "^", "\[Alpha]"}], ",", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
